{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": 0,
    "react/jsx-one-expression-per-line": 0,
    "import/no-unresolved": 0,
    "jsx-a11y/anchor-is-valid": 0,
  }
}